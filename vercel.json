{
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "build/index.js"
        }
    ],
    "crons": [
        {
          "path": "build/schedulers",
          "schedule": "30 5 * * *"
        }
      ]
  }